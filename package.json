{
    "name": "coding-ninjas-2",
    "version": "1.0.0",
    "description": "Coding Ninjas II - gifted with second sight",
    "author": "Christopher Stock <email@christopherstock.de>",
    "license": "MIT",
    "main": "dist/index.html",
    "homepage": "https://github.com/christopherstock/coding-ninjas-ii",
    "repository": "git@github.com:christopherstock/coding-ninjas-II.git",
    "bugs": "https://github.com/christopherstock/coding-ninjas-II/issues",
    "scripts": {
        "typescript:compile": "tsc",
        "webpack:dev": "webpack --mode=development --watch",
        "webpack:production": "webpack --mode=production --watch",
        "webpack:devserver": "webpack-dev-server --mode=development",
        "typedoc:generate": "typedoc --out doc --listInvalidSymbolLinks --name \"Coding Ninjas II - gifted with second sight, (c) 2023 Mayflower GmbH\" src/typescript",
        "eslint:check": "eslint \"src/**/*.{ts,tsx}\"",
        "eslint:fix": "eslint --fix \"src/**/*.{ts,tsx}\"",
        "test:all": "mocha --require ts-node/register --require ignore-styles --require src/test/index.ts src/test/**/*.ts src/test/**/*.tsx",
        "test:jest": "jest",
        "test:jest-coverage": "jest --coverage"
    },
    "devDependencies": {
        "@types/chai": "4.1.7",
        "@types/cucumber": "3.2.1",
        "@types/enzyme": "3.10.3",
        "@types/fpsmeter": "0.3.29",
        "@types/jest": "25.1.2",
        "@types/matter-js": "0.14.1",
        "@types/moment": "2.13.0",
        "@types/node": "9.6.48",
        "@types/react": "16.9.49",
        "@types/react-dom": "16.9.8",
        "@types/webpack": "4.4.29",
        "@typescript-eslint/eslint-plugin": "3.7.0",
        "@typescript-eslint/eslint-plugin-tslint": "3.7.0",
        "@typescript-eslint/parser": "3.7.0",
        "awesome-typescript-loader": "5.2.1",
        "chai": "4.2.0",
        "css-loader": "0.28.11",
        "cucumber": "4.0.0",
        "enzyme": "3.10.0",
        "enzyme-adapter-react-16": "1.14.0",
        "eslint": "7.5.0",
        "eslint-plugin-import": "2.22.0",
        "eslint-plugin-prefer-arrow": "1.2.2",
        "ignore-styles": "5.0.1",
        "jest": "25.1.0",
        "less": "3.11.1",
        "less-loader": "4.1.0",
        "mocha": "5.2.0",
        "react-test-renderer": "16.13.1",
        "source-map-loader": "0.2.4",
        "style-loader": "0.20.3",
        "ts-jest": "24.1.0",
        "tslint": "6.0.0",
        "ts-node": "7.0.1",
        "typedoc": "0.16.9",
        "typescript": "3.7.5",
        "webpack": "4.41.6",
        "webpack-cli": "3.3.10",
        "webpack-dev-server": "3.10.3"
    },
    "dependencies": {
        "antd": "3.26.9",
        "fpsmeter": "0.3.1",
        "matter-js": "0.14.1",
        "moment": "2.24.0",
        "rc-scroll-anim": "2.7.4",
        "rc-tween-one": "2.6.8",
        "react": "16.13.1",
        "react-dom": "16.13.1",
        "wowjs": "1.1.3"
    },
    "jest": {
        "verbose": true,
        "transform": {
            ".(ts|tsx)": "ts-jest"
        },
        "testRegex": "(/src/test/.*\\.(test|spec))\\.(ts|tsx|js|jsx)$",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx"
        ],
        "testURL": "http://localhost/",
        "setupFiles": [
            "<rootDir>/src/test/index.ts"
        ]
    }
}
