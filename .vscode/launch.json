{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node-terminal",
            "name": "webpack:devserver",
            "command": "npm run webpack:devserver",
            "request": "launch"
        },
        {
            "type": "pwa-chrome",
            "name": "Coding Ninjas II",
            "request": "launch",
            "url": "http://localhost:1234",
            "webRoot": "${workspaceRoot}",
            "sourceMaps": true,
            "trace": true
        }
    ]
}
